<Window x:Class="Product_Incentive_Report.WindowRenameOutletGroup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rename Outlet Group" Height="152" Width="300" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" ShowInTaskbar="False" Closed="Window_Closed">
    <Grid Height="113">
        <Label Content="Old Name" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="labelOldName" VerticalAlignment="Top" />
        <Label Content="New Name" Height="28" HorizontalAlignment="Left" Margin="12,41,0,0" Name="labelNewName" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="92,12,0,0" Name="textBoxOldName" VerticalAlignment="Top" Width="120" IsReadOnly="True" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="92,41,0,0" Name="textBoxNewName" VerticalAlignment="Top" Width="120" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="48,75,0,0" Name="buttonOK" VerticalAlignment="Top" Width="75" Click="buttonOK_Click" IsDefault="True" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="155,75,0,0" Name="buttonCancel" VerticalAlignment="Top" Width="75" Click="buttonCancel_Click" IsCancel="True" />
    </Grid>
</Window>
