<Window x:Class="Product_Incentive_Report.WindowRenameProductGroup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rename Product Group" Height="147" Width="300" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" ShowInTaskbar="False" Closed="Window_Closed">
    <Grid Height="108">
        <Label Content="Old Name" Height="28" HorizontalAlignment="Left" Margin="10,10,0,0" Name="labelOldName" VerticalAlignment="Top" />
        <Label Content="New Name" Height="28" HorizontalAlignment="Left" Margin="10,39,0,0" Name="labelNewName" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" IsReadOnly="True" Margin="90,10,0,0" Name="textBoxOldName" VerticalAlignment="Top" Width="120" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="90,39,0,0" Name="textBoxNewName" VerticalAlignment="Top" Width="120" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="46,73,0,0" Name="buttonOK" VerticalAlignment="Top" Width="75" Click="buttonOK_Click" IsDefault="True" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="153,73,0,0" Name="buttonCancel" VerticalAlignment="Top" Width="75" Click="buttonCancel_Click" IsCancel="True" />
    </Grid>
</Window>
